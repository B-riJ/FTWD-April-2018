<h1>Log Into Your Account</h1>

<form action="/login" method="post">

<label> Username</label>
  <input type="text" name="username">

  <label>Password</label>
  <input type="password" name="password">

  <button>Submit</button>

</form>

<br>
<br>
{{#if message}}
<div class="message">
  {{message}}
</div>
{{/if}}
